views:
  - type: table
    name: 表格
    filters:
      and:
        - file.path.startsWith("00-前端")
        - file.ext == "md"
    order:
      - file.name
      - file.folder
      - file.tags
    sort:
      - property: file.folder
        direction: ASC
      - property: file.tags
        direction: ASC
    columnSize:
      file.folder: 406
  - type: cards
    name: 视图
    order:
      - file.name
      - file.basename
      - file.ctime
      - file.backlinks
      - title
